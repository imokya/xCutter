{
  "name": "xcutter",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "nodemon --watch main.js --exec electron .",
    "postinstall": "electron-builder install-app-deps",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.electron.xcutter",
    "productName": "xCutter",
    "copyright": "copyright Â©2019 xingway",
    "directories": {
      "output": "dist"
    },
    "files": ["**/*", "build/*"],
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "npmRebuild": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imokya/xcutter.git"
  },
  "keywords": [
    "xcutter",
    "landing page",
    "unique"
  ],
  "author": "xingway",
  "license": "ISC",
  "devDependencies": {
    "electron": "^6.0.12",
    "nodemon": "^1.19.3",
    "electron-builder": "^21.2.0",
    "electron-rebuild": "^1.8.6"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "fs-extra": "^8.1.0",
    "jquery": "^3.4.1",
    "pixi.js": "^5.1.5",
    "popper.js": "^1.16.0",
    "pretty": "^2.0.0",
    "sharp": "^0.23.1"
  }
}
